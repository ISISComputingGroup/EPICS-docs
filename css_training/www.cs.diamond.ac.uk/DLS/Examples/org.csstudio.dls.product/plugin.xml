<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.csstudio.startup.application.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.csstudio.startup.module">
		<startupParameters
			class="org.csstudio.utility.product.StartupParameters">
		</startupParameters>
		<project
			class="org.csstudio.startup.module.defaults.DefaultProject">
		</project>
		<workbench
			class="org.csstudio.utility.product.Workbench">
		</workbench>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.dls.product.id1"
            name="DLS">
         <property
               name="appName"
               value="DLS">
         </property>
      </product>
   </extension>

</plugin>
